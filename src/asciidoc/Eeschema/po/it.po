#: Eeschema_chapter_05.adoc:446
#, no-wrap
msgid "Sheet title block"
msgstr ""

#. type: Plain text
#: Eeschema_chapter_05.adoc:450
msgid ""
"The title block is edited with the tool image:"
"images/20000008000002470000022DA00BC8FE."
"png[20000008000002470000022DA00BC8FE_png]."
msgstr ""

#. type: Plain text
#: Eeschema_chapter_05.adoc:452
msgid ""
"image:images/10000000000002C100000230033A432A."
"png[10000000000002C100000230033A432A_png]"
msgstr ""

#. type: Plain text
#: Eeschema_chapter_05.adoc:454
msgid "The complete title block will be as follows."
