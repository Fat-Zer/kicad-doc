msgid "*--param name value*"
msgstr ""

# d6999b059595464db229ad7aa79f39b6
#: ../../source/eeschema_chapter14.rst:1338
msgid ""
"Pass a parameter of name *name* and value *value* to the stylesheet. You may "
"pass multiple name/value pairs up to a maximum of 32. If the value being "
"passed is a string rather than a node identifier, use --stringparam instead."
msgstr ""

# 4e16d237b43f4cc9b7eeda6e1dbe1d9a
#: ../../source/eeschema_chapter14.rst:1343
msgid "*--stringparam name value*"
msgstr ""

# 992797fda47f4b15b21cad6b54b37ccf
#: ../../source/eeschema_chapter14.rst:1345
msgid ""
"Pass a paramenter of name *name* and value *value* where *value* is a string "
"rather than a node identifier. (Note: The string must be utf-8.)"
msgstr ""

# 5a7576b1bbcd4d91a27bb7b8ae1ad869
#: ../../source/eeschema_chapter14.rst:1348
msgid "*--nonet*"
msgstr ""

# 9c52204985974fa99b3bade21f47b662
#: ../../source/eeschema_chapter14.rst:1350
