#: ../../source/eeschema_chapter14.rst:1332
msgid "*--html*"
msgstr ""

# a9563847991c4b289bc43fb239489675
#: ../../source/eeschema_chapter14.rst:1334
msgid "The input document is an HTML file."
msgstr ""

